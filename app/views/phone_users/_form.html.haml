= form_for(@phone_user) do |f|
  - if @phone_user.errors.any?
    #error_explanation
      %h2
        = pluralize(@phone_user.errors.count, "error")
        prohibited this phone_user from being saved:
      %ul
        - @phone_user.errors.full_messages.each do |message|
          %li= message
  .field
    = f.label :last_name
    %br/
    = f.text_field :last_name
  .field
    = f.label :first_name
    %br/
    = f.text_field :first_name
  .field
    = f.label :second_name
    %br/
    = f.text_field :second_name
  .actions
    = f.submit
